@page "/"
@namespace maps.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>maps</title>
    <base href="~/" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
   
    <!--GridStack CSS-->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/gridstack.js/0.4.0/gridstack.min.css" />

    @*side nav*@

</head>
<body>
    <app>
        @(await Html.RenderComponentAsync<App>(RenderMode.Static))
    </app>

    <!--Jquery-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>

    <!--Loadash required for Gridstack-->
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/0.10.0/lodash.min.js"></script>

    <!--GridStack-->
    <script type="text/javascript" src='//cdnjs.cloudflare.com/ajax/libs/gridstack.js/0.4.0/gridstack.min.js'></script>


    <!-- GridStack 0.5-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.11.0/jquery-ui.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/core-js/2.4.1/shim.min.js"></script>
    <script src="lib/gridstack/gridstack.js"></script>
    <script src="lib/gridstack/gridstackjqueryUI.js"></script>



    <!--Amcharts 4-->
    <script src="//www.amcharts.com/lib/4/core.js"></script>
    <script src="//www.amcharts.com/lib/4/charts.js"></script>
    <script src="//www.amcharts.com/lib/4/themes/animated.js"></script>
    <script src="//www.amcharts.com/lib/4/themes/kelly.js"></script>
    <script src="js/xychartconfig.js"></script>

    <!--Scripts-->
    <!--Leaflets-->
    <script src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js"></script>
    <script src="https://unpkg.com/leaflet@1.0.2/dist/leaflet.js"></script>
    <!--Maps-->
    <script src="lib/mapscontextmenulib.js"></script>
    <script src="js/mapsintegration.js"></script>
    <!--GridStack-->
    <script src="lib/gridstack.js"></script>
    <script src="js/grid.js"></script>



    <script src="_framework/blazor.server.js"></script>



</body>
</html>
